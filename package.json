{
  "name": "biome-rs-npm",
  "version": "1.9.0",
  "type": "module",
  "description": "A toolchain for web projects, aimed to provide functionalities to maintain them. Biome offers formatter and linter, usable via CLI and LSP",
  "author": "Davlatjon Shavkatov <opensource@dalisoft.uz>, Biomejs Team",
  "files": [
    "bin.sh"
  ],
  "bin": {
    "biome": "bin.sh"
  },
  "engines": {
    "node": ">=18.19.1"
  },
  "keywords": [
    "javascript",
    "css",
    "json",
    "formatter",
    "typescript",
    "web",
    "static-code-analysis",
    "jsx",
    "linter"
  ],
  "optionalDependencies": {
    "@biomejs/cli-win32-x64": "1.8.3",
    "@biomejs/cli-win32-arm64": "1.8.3",
    "@biomejs/cli-darwin-x64": "1.8.3",
    "@biomejs/cli-darwin-arm64": "1.8.3",
    "@biomejs/cli-linux-x64": "1.8.3",
    "@biomejs/cli-linux-arm64": "1.8.3",
    "@biomejs/cli-linux-x64-musl": "1.8.3",
    "@biomejs/cli-linux-arm64-musl": "1.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalisoft/biome-rs-npm.git"
  },
  "bugs": {
    "url": "https://github.com/dalisoft/biome-rs-npm/issues"
  },
  "homepage": "https://github.com/dalisoft/biome-rs-npm#readme",
